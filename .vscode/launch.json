{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:80/ADISE24_ataxxangelos/game.html",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "skipFiles": ["<node_internals>/**"],
            "sourceMapPathOverrides": {
                "webpack:///src/*": "${webRoot}/**",
                "webpack:///./~/*": "${webRoot}/node_modules/*"
            }
        }
    ]
}
